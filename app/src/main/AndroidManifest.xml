<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.icddrb.standard_v3"
    android:installLocation="preferExternal">


    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="28"/>
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" android:required="true" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GPS" />

    <application
        android:name="apps_font.CustomFontApp"
        android:allowBackup="true"
        android:icon="@drawable/logo_app"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/logo_app"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/CustomTheme"
        android:requestLegacyExternalStorage="true"
        android:usesCleartextTraffic="true">
        <activity
            android:name="org.icddrb.standard_v3.Chart_Activity"
            android:exported="false" />
        <activity
            android:name="org.icddrb.standard_v3.PreparingDatabase"
            android:exported="false" />
        <activity
            android:name="org.icddrb.standard_v3.RebuildDatabaseForm"
            android:exported="false"
            android:label="@string/title_activity_rebuild_database_form"
            android:theme="@style/CustomTheme.NoActionBar" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true" >
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <activity
            android:name="org.icddrb.standard_v3.SettingForm"
            android:label="Device Setting" /> <!-- Android Manifest Code -->
        <activity
            android:name="org.icddrb.standard_v3.Splash_Screen"
            android:exported="true"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- Activity -->
        <activity android:name="org.icddrb.standard_v3.LoginActivity" />
        <activity android:name="org.icddrb.standard_v3.data_form_master" />
        <activity android:name="org.icddrb.standard_v3.data_from_content" />
        <activity android:name="print.PrintExampleActivity" />
        <activity android:name="org.icddrb.standard_v3.Scan_Barcode_Activity" />
        <service android:name="org.icddrb.standard_v3.DataSyncService" />
        <service android:name="org.icddrb.standard_v3.Sync_Service" />
        <service android:name="org.icddrb.standard_v3.GPSService" />
        <service android:name="org.icddrb.standard_v3.DatabaseFileSync_Service" />
        <service android:name="org.icddrb.standard_v3.Sync_Service_DatabaseStructure" />

        <activity android:name="forms_system.DeviceList" />
        <activity android:name="forms_system.DeviceList_list" />
        <activity android:name="forms_activity.Mapping_Household_list" />
        <activity android:name="forms_activity.Mapping_Landmark_list" />
        <activity android:name="forms_activity.HouseholdVisit" />
        <activity android:name="forms_activity.Household_list" />
        <activity android:name="forms_activity.Bari" />
        <activity android:name="forms_activity.Household" />
        <activity android:name="gps.GPS_Data" />
        <activity android:name="gps.GPS_Landmark_Data" />
        <activity android:name="forms_activity.Indicator_List" />
        <activity android:name="org.icddrb.standard_v3.Bottom_Navigation_MainActivity" />
        <activity android:name="forms_activity.data_registration" />
    </application>
</manifest>